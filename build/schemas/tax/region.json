{
  "$schema": "http://json-schema.org/draft/2020-12/schema",
  "$id": "https://gobl.org/draft-0/tax/region",
  "$ref": "#/$defs/Region",
  "$defs": {
    "Category": {
      "properties": {
        "code": {
          "type": "string",
          "title": "Code"
        },
        "name": {
          "$ref": "https://gobl.org/draft-0/i18n/string",
          "title": "Name"
        },
        "desc": {
          "$ref": "https://gobl.org/draft-0/i18n/string",
          "title": "Description"
        },
        "retained": {
          "type": "boolean",
          "title": "Retained"
        },
        "defs": {
          "items": {
            "$ref": "#/$defs/Def"
          },
          "type": "array",
          "title": "Definitions"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "code",
        "name",
        "defs"
      ]
    },
    "Def": {
      "properties": {
        "code": {
          "type": "string",
          "title": "Code"
        },
        "name": {
          "$ref": "https://gobl.org/draft-0/i18n/string",
          "title": "Name"
        },
        "desc": {
          "$ref": "https://gobl.org/draft-0/i18n/string",
          "title": "Description"
        },
        "values": {
          "items": {
            "$ref": "#/$defs/Value"
          },
          "type": "array",
          "title": "Values",
          "description": "Set of values ordered by date that determine what rates to apply since when."
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "code",
        "name",
        "values"
      ]
    },
    "Region": {
      "properties": {
        "code": {
          "type": "string",
          "title": "Code"
        },
        "name": {
          "$ref": "https://gobl.org/draft-0/i18n/string",
          "title": "Name"
        },
        "categories": {
          "items": {
            "$ref": "#/$defs/Category"
          },
          "type": "array",
          "title": "Categories"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "code",
        "name",
        "categories"
      ]
    },
    "Value": {
      "properties": {
        "since": {
          "$ref": "https://gobl.org/draft-0/org/date",
          "title": "Since"
        },
        "percent": {
          "$ref": "https://gobl.org/draft-0/num/percentage",
          "title": "Percent"
        },
        "disabled": {
          "type": "boolean",
          "title": "Disabled"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "percent"
      ]
    }
  }
}